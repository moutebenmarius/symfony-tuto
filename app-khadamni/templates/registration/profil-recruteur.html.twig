{% extends 'khadamni/index.html.twig' %}

{% block title %}Inscription!{% endblock %}

{% block dynamic %}
        <section class="mt-4">
            <div class="container mt-4">
                
    
                <div class="row">
                    <div class="col-12">
                        <h2 class="contact-title text-center mb-2">Inscription</h2>
                    </div> 
                </div>
                {{ form_start(form) }}
                <div class="row">
                    
                    <div class="col-12">
                        <div class="card">
                             <div class="card-header bg-primary text-white text-center">
                                    Complétér votre Profile
                             </div>

                             <div class="card-body">
                                    
                                    <div class="form-group">
                                        {{ form_widget(form) }}
                                    </div>
                                    <div class="form-group">
                                        <button type="submit" class="btn btn-primary">Inscrire</button>
                                    </div>
                             </div>
                        </div>
                                     
                                    
                                   
                                    
                            
                    </div> 
                    
                </div>
                {{ form_end(form) }}
                
            </div>
        </section>

{% endblock %}
