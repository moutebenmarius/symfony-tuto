<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title>
			{% block title %}Bienvenue !
			{% endblock %}
		</title>
		{# Run `composer require symfony/webpack-encore-bundle`
				           and uncomment the following Encore helpers to start using Symfony UX #}
		{% block stylesheets %}
			<link
			href="{{ asset('assets/css/reset.css') }}" rel="stylesheet" type="text/css">
			<!-- Bootstrap -->
			<link
			href="{{ asset('assets/assets/bootstrap/css/bootstrap.min.css') }}" rel="stylesheet" type="text/css">
			<!-- Font Awesome -->
			<link
			href="{{ asset('assets/assets/font-awesome/css/font-awesome.min.css') }}" rel="stylesheet" type="text/css">
			<!-- Iconmoon -->
			<link
			href="{{ asset('assets/assets/iconmoon/css/iconmoon.css') }}" rel="stylesheet" type="text/css">
			<!-- Lightbox CSS -->
			<link
			href="{{ asset('assets/assets/lightbox/css/lightbox.css') }}" rel="stylesheet" type="text/css">
			<!-- Owl Carousel -->
			<link
			href="{{ asset('assets/assets/owl-carousel/css/owl.carousel.min.css') }}" rel="stylesheet" type="text/css">
			<!-- Animate -->
			<link
			href="{{ asset('assets/css/animate.css') }}" rel="stylesheet" type="text/css">
			<!-- Aos -->
			<link
			href="{{ asset('assets/assets/aos/aos.css') }}" rel="stylesheet" type="text/css">
			<!-- Custom Style -->
			<link href="{{ asset('assets/css/custom.css') }}" rel="stylesheet" type="text/css">
			<link href="{{ asset('assets/calender/main.min.css') }}" rel="stylesheet" type="text/css">
			<link href="{{ asset('assets/calender/main.min.js') }}" rel="stylesheet" type="text/css">
			<script>

				document.addEventListener('DOMContentLoaded', function () {
var calendarEl = document.getElementById('calendar');
var calendar = new FullCalendar.Calendar(calendarEl, {initialView: 'dayGridMonth'});
calendar.render();
});
			</script>
		{% endblock %}


	</head>
	<body>
		{% block body %}{% endblock %}
		{% block javascripts %}
			<script src="{{ asset('assets/js/jquery.min.js') }}"></script>
			<!-- Bootstrap JS -->
			<script src="{{ asset('assets/assets/bootstrap/js/bootstrap.min.js') }}"></script>
			<!-- Lightbox JS -->
			<script src="{{ asset('assets/assets/lightbox/js/lightbox.js') }}"></script>
			<!-- Owl Carousal JS -->
			<script src="{{ asset('assets/assets/isotope/js/isotope.min.js') }}"></script>
			<!-- Owl Carousal JS -->
			<script src="{{ asset('assets/assets/owl-carousel/js/owl.carousel.min.js') }}"></script>
			<!-- Aos JS -->
			<script src="{{ asset('assets/assets/aos/aos.js') }}"></script>
			<!-- CounterUp JS -->
			<script src="{{ asset('assets/assets/counterup/counterup.min.js') }}"></script>
			<script src="{{ asset('assets/assets/counterup/waypoints.min.js') }}"></script>
			<!-- Custom JS -->
			<script src="{{ asset('assets/js/custom.js') }}"></script>
		{% endblock %}
	</body>
</html>
